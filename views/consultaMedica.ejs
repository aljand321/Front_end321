<!DOCTYPE html>
<html lang="es">
<head>
	<title>Inicio</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<%- include("partials/head") -%>
</head>
<body>
	<!-- SideBar -->
	<%- include("partials/menudoc") -%>
	<!-- Content page-->
<section class="full-box dashboard-contentPage">
	<!-- NavBar -->
	<nav class="full-box dashboard-Navbar">
			<ul class="full-box list-unstyled text-right">
				<li class="pull-left">
					<a href="#!" class="btn-menu-dashboard"><i class="zmdi zmdi-more-vert"></i></a>
				</li>
			
			</ul>
		</nav>
		<!-- Content page -->
		<div class="container-fluid">
			<div class="page-header">
			  <h1 class="text-titles"><i class="zmdi zmdi-account-calendar zmdi-hc-fw"></i> Consulta <small>Médica</small></h1>
			</div>
		</div>
		<div class="container-fluid">
			<div class="row">
				<div class="col-xs-12">
					
					<div class="tab-content">
						<div class="tab-pane fade active in" id="newSchool">
							<div class="container-fluid">
								<div class="row">
										
										<div class="container-fluid">
											<div class="col-md-12 panel-body">
												
                         <!-- daros personales-->
                            	<div class="col-md-6">
                                <p class="lead">Datos Personales </p>
                              </div>
                              <div class="col-md-6">
                                  <div class="form-group label-floating">
																		<label class="control-label">N° de Historia</label>
																		<input class="form-control" type="text" name="apPaterno">
																	</div>
                              </div>
													
																													
															<!-- lado hizquierdo -->
															
															<div class="col-md-6">
																<div class="form-group label-floating">
																	<label class="control-label">Apellido Paterno</label>
																	<input class="form-control" type="text" name="apPaterno">
																</div>
																<div class="form-group label-floating">
																		<label class="control-label">Nombre</label>
																		<input class="form-control" type="text" name="nombre">
																</div>
																<div class="form-group label-floating">
																		<label class="control-label">Fecha de Nacimiento</label>
																		<input class="form-control" type="text" name="fechNacimien">
																</div>
																<div class="form-group label-floating">
																		<label class="control-label">Estado Civil</label>
																		<select class="form-control" name="estadoCivil">
																			<option></option>
																			<option>Casado(a)</option>
																			<option>Divorciado(a)</option>
																			<option>Soltero(a)</option>
																			<option>Viudo(a)</option>
																			</select>
																	</div>
															</div>
															<!--   lado derecho-->
															<div class="col-md-6">
																<div class="form-group label-floating">
																	<label class="control-label">Apellido Materno</label>
																	<input class="form-control" type="text" name="apMaterno">
																</div>
																<div class="form-group label-floating">
																		<label class="control-label">CI</label>
																		<input class="form-control" type="text" name="ci">
																</div>
																<div class="checkbox" style=" height: 70px;">
																	<h4 class="text-titles"> Sexo</h4>			 
																	<input type="radio" name="sexo" id="hombre">
																	<label for="hombre">Hombre</label>
																	<input type="radio" name="sexo" id="mujer">
																	<label for="mujer">Mujer</label>
																</div>
																<div class="form-group label-floating">
																		<label class="control-label">Ocupación</label>
																		<input class="form-control" type="text" name="ocupacion">
																</div>
															</div>
                            							 <!-- botones  -->
                                                    
																
                                            </div>
                                          <!-- menu-->
                                            <div class="container-fluid">
                                                    <div class="row">
                                                        <div class="col-xs-12">
                                                            <ul class="nav nav-tabs" style="margin-bottom: 15px;">
                                                                 
                                                                  <li class="active"><a href="#ListPaciente" data-toggle="tab"><i class="zmdi zmdi-assignment"></i> Datos generales</a></li>
                                                                  <li class="active"><a href="#ListPaciente" data-toggle="tab"><i class="zmdi zmdi-assignment"></i> Antecendentes Personales</a></li>
                                                                  <li class="active"><a href="#ListPaciente" data-toggle="tab"><i class="zmdi zmdi-assignment"></i> Antecentes Famliares</a></li>
                                             
                                                              
                                                               
                                                            </ul>
                                                        </div>
                                                    </div>
																						</div>
																						<!-- diagnostico-->
																						<div class="form-group label-floating col-md-12">
																							<label class="control-label">Diagnostico</label>
																							<textarea  class="textarea text-center" name="comentarios" rows="10" cols="80"></textarea>
																						
																					</div>

																						<!--formularios-->
																						
																					  <div class="container-fluid">
                                                    <div class="row">
                                                        <div class="col-xs-12">
																													<button type="button" class="btn btn-raised btn-success" onclick=location="Papeleta_Inter">Papeleta de Internacion</button>
																													<button type="button" class="btn btn-raised btn-success" onclick=location="recetas">Receta</button>
																												
                                                        </div>
                                                    </div>
                                            </div>
                                            


                                               
                                            </div>

												<table class="table table-hover text-center">
													<thead>
												</table>
												<p class="text-center">
													<button type="reset" class="btn btn-info" style="margin-right: 20px;"><i class="zmdi zmdi-roller"></i> &nbsp;&nbsp; Limpiar</button>
													<button type="submit" class="btn btn-danger"><i class="zmdi zmdi-floppy"></i> &nbsp;&nbsp; Guardar</button>
													<button type="submit" class="btn btn-outline-danger"><i class="zmdi zmdi-download"></i> &nbsp;&nbsp; Imprimir</button>
												</p>
										</div>
								</div>
							</div>
									
									
						</div>
	          		</div>
				</div>
			</div>
		</div>
</section>
	<!--====== Scripts -->
	<%- include("partials/scripts") -%>
</body>
</html>