<!DOCTYPE html>
<html lang="es">
<head>
	<title>Consulta Medica</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<%- include("partials/head") -%>
</head>
<body class=" full-box dashboard-sideBar-imagg cover ">
	<!-- SideBar -->
	<%- include("partials/menudoc") -%>

	<li>
		<a href="/medico/renderVistaDocotr/<%=consultamedica%>">
			<i class="zmdi zmdi-view-dashboard zmdi-hc-fw"></i> Home
		</a>
	</li>

    <li>
		<a href="#!" class="btn-sideBar-SubMenu">
			<i class="zmdi zmdi-file-plus zmdi-hc-fw"></i> Consultas externas <i class="zmdi zmdi-caret-down pull-right"></i>
		</a>
		<ul class="list-unstyled full-box">
			<li><a href="/medico/listfalseConsulta/<%=consultamedica%>"><i class="zmdi zmdi-accounts-list zmdi-hc-fw"></i>Listas de citas</a></li>
					  
		</ul>
	</li>
	<%- include("partials/footerDocotr") -%>
	<!-- Content page-->
<section class="full-box dashboard-contentPage dashboard-sideBar-cttt ">
	<!-- NavBar -->
	<nav class="full-box dashboard-Navbar ">
		<ul class="full-box list-unstyled text-right dashboard-sideBar-ctttt ">
			<li class="pull-left">
		    	<a href="#!" class="btn-menu-dashboard"><i class="zmdi zmdi-more-vert"></i></a>
			</li>
		</ul>
	</nav>
	<!-- Content page -->
	<div class="container-fluid">
		<div class="page-header">
			<h1 class="text-titles"><i class="zmdi zmdi-file-plus zmdi-hc-fw"></i>Consulta Externa</h1>
		</div>
	</div>
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-12">
				<ul class="nav nav-tabs" style="margin-bottom: 15px;">
				  	<li class="active"><a href="#ListPaciente" data-toggle="tab"><i class="zmdi zmdi-assignment"></i> Pacientes no Atendidos </a></li>
					<li><a href="#Pacientes" data-toggle="tab"><i class="zmdi zmdi-assignment"></i> Pacientes atendidos</a></li>
				</ul>
				<!--Tabla de Pacientes Atendidos-->
				<div id="myTabContent" class="tab-content">
					<div class="tab-pane fade active in" id="ListPaciente">
						<div class="table-responsive" >
							<div id="correo">
								<div class="entrada">
									<button type="button" value="Imprimir" onclick="window.print()" class="btn btn-success btn-raised btn-lg"><i class="zmdi zmdi-print"></i> &nbsp;&nbsp; Imprimir</button>
								</div>
								<div class="entrada">Buscar <i class="zmdi zmdi-search"></i><br>
									<input type="text" id="searchInput" placeholder="Buscar.."><br>
								</div>
							</div>
							<table class="fl-table" id="myTable">
                            	<thead>
									<tr>
									<th class="text-center">#</th>
									<th class="" WIDTH="15%">N° de historia </th>
									<th class="" WIDTH="20%">Nombre del Paciente</th>
									<th class="" WIDTH="20%">Apellido paterno</th>
									<th class="" WIDTH="20%">Apellido materno</th>	
									<th class="" WIDTH="20%">Hora de Atencion</th>	
									<th class="text-center" WIDTH="20%">Reg.Consulta </th>
                  					</tr>
								</thead>
								<tbody>
								  <% if(resp != null) { %>
									<% for(var i=0; i< resp.length; i++) { %>
										<tr>
											<td><%= i + 1 %></td>
											<td><%=resp[i].codigo_p %></td>
											<td><%=resp[i].Paciente.nombre %></td>
											<td><%=resp[i].Paciente.apellidop %></td>
											<td><%=resp[i].Paciente.apellidom %></td>
											<td><%=resp[i].hora.split("/")[0] %></td>
											<td class="text-center"><a href="/medico/consulta/<%=resp[i].codigo_p%>/<%=resp[i].id%>" class="btn btn-success btn-raised btn-xs"><i class="zmdi zmdi-edit zmdi-hc-lg"></i></a></td>
											
											
										</tr>
										<% } %>
								  	<% } else { %>									
									<tr>
									<td> NO HAY CONENCCION CON LA BASE DE DATOS </td>
									</tr>
								<% } %>
								</tbody>
							</table>
						</div>
					</div>
					<!-- lista de pacientes atendidos-->
					<div class="tab-pane fade" id="Pacientes">
						<div class="table-responsive" >
							<div id="correo">
								<div class="entrada">
									<button type="button" value="Imprimir" onclick="window.print()" class="btn btn-success btn-raised btn-lg"><i class="zmdi zmdi-print"></i> &nbsp;&nbsp; Imprimir</button>
								</div>
								<div class="entrada">Buscar <i class="zmdi zmdi-search"></i><br>
									<input type="text" id="searchInput" placeholder="Buscar.."><br>
								</div>
							</div>
							<table class="fl-table" id="myTable">
							 	<thead>
								  	<tr>
								  	<th class="text-center">#</th>
							  		<th class="" WIDTH="15%">N° de historia </th>
							  		<th class="" WIDTH="20%">Nombre del Paciente</th>
							  		<th class="" WIDTH="20%">Apellido paterno</th>
							  		<th class="" WIDTH="20%">Apellido materno</th>	
							  		<th class="" WIDTH="20%">Hora de Atencion</th>	
							  		<th class="text-center" WIDTH="20%">Modificar</th>
									</tr>
							  	</thead>
							  	<tbody>
									<% if(falseCita != null) { %>
							
										<% for(var i=0; i< falseCita.length; i++) { %>
											<tr>
												<td><%= i + 1 %></td>
												<td><%=falseCita[i].codigo_p %></td>
												<td><%=falseCita[i].Paciente.nombre %></td>
												<td><%=falseCita[i].Paciente.apellidop %></td>
												<td><%=falseCita[i].Paciente.apellidom %></td>
												<td><%=falseCita[i].hora.split("/")[0] %></td>
												<td class="text-center"><a href="/medico/consulta/<%=falseCita[i].codigo_p%>/<%=falseCita[i].id%>" class="btn btn-success btn-raised btn-xs"><i class="zmdi zmdi-edit zmdi-hc-lg"></i></a></td>
												
												
											</tr>
											<% } %>
										  <% } else { %>
										<tr>
											<td> NO HAY CONENCCION CON LA BASE DE DATOS </td>
										</tr>
										<% } %>
							  </tbody>
						  </table>
						</div>
					</div>
				</div>
			</div>	
		</div>
	</div>
</section>
	<!--====== Scripts -->
	<%- include("partials/scripts") -%>
		
</body>
</html>