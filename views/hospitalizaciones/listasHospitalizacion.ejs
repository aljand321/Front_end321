							<h2 class="content-header "><span>&nbsp;&nbsp;&nbsp;</span>Ordenes de Hospitalización</h2>
						</div>	
					</div> 
					<div class="main-content">
						<section class="panel default blue_title h2">
							<div class="panel-body page-content">
								<ul class="nav nav-tabs" id="myTab">
									<li class=" col-lg-6 active active  text-center"><a data-toggle="tab" href="#list"> <i class="fa fa-user "></i><span>&nbsp;&nbsp;&nbsp;</span>Lista de Pacientes </a></li>
									<li class="col-lg-6 text-center" ><a  data-toggle="tab" href="#listFalse" > <i class="fa fa-medkit "></i><span>&nbsp;&nbsp;&nbsp;</span>Pacientes Atendidos</a></li>
								</ul>
								<div class="tab-content" id="myTabContent">
									<div id="list" class="tab-pane fade in active">
										<table class="display table table-bordered table-striped" id="dynamic-tablee">
											<thead>
												<tr>
												<th class="text-center">#</th>
												<th class="" WIDTH="15%">N° de historia </th>
												<th class="" WIDTH="20%">Fecha</th>	
												<th class="" WIDTH="20%">Motivo de Ingreso</th>	
												<th class="" WIDTH="20%">Proviene</th>	
												<th class="text-center" WIDTH="20%">Registrar</th>
												</tr>
											</thead>
											<tbody>
												<% if(resp != null) { %>
													<% for(var i=0; i< resp.length; i++) { %>
														<tr>
															<td><%= i + 1 %></td>
															<td><%=resp[i].Historial %></td>
															<td><%=resp[i].fechaIngreso %></td>
															<td><%=resp[i].diagnostico %></td>
															<td><%=resp[i].tipoConsulta %></td>
															<td class="text-center" >
																<a href="/internaciones/only_pInternacion/<%=resp[i].id%>/<%=resp[i].id_especialidad%>/<%=resp[i].tipoConsulta%>/<%=resp[i].Historial %>/<%=data_doc.data.token_id%>"  data-toggle="tooltip" class="btn  tooltips" data-original-title="Registrar Hospitalización"><i class="fa fa-eye text-muted" style="color:rgb(19, 125, 224)"></i></a><span>&nbsp;</span>
															</td>
														</tr>
													<% } %>
												<% } else { %>
													<tr>
														<td> NO HAY CONENCCION CON LA BASE DE DATOS </td>
													</tr>
												<% } %>							
											</tbody>
										</table>
									</div>
									<div class="tab-pane fade" id="listFalse">
