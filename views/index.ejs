		   </header>
		   <!--     End  header Content  -->
		   <br>
		   <br>
		   <br>
		   <div class="field-set">
			 
			  <!--   user name -->
				 <span class="input-item">
					<i class="zmdi zmdi-account-circle"></i>
				 </span>
				<!--   user name Input-->
				 <input class="form-input" id="txt-input" type="email" placeholder="@UserName" required>
			 
			  <br>
			  <br>
			 
				   <!--   Password -->
			 
			  <span class="input-item">
				<i class="zmdi zmdi-key"></i>
			   </span>
			  <!--   Password Input-->
			  <input class="form-input" type="password" placeholder="Password" id="pwd"  name="password" required>
			 
		<!--      Show/hide password  -->
			 <span>
				<i class="zmdi zmdi-eye" aria-hidden="false"  type="button" id="eye"></i>
			 </span>
			 
			 
			  <br>
			  <br>
		<!--        buttons -->
		<!--      button LogIn -->
			  <!---->
			  <div class="form-group text-center">
				<button class="log-in"> INICIAR </button>
			</div>
			<div class="form-group text-center">
				<button type="button" class="log-in" onclick=location="/HomeVistDoctor">entra doctor</button> &nbsp;&nbsp; 
			</div>
			<div class="form-group text-center">
				<button type="button" class="log-in" onclick=location="/cuaderno/cuaderno">Cuaderno</button> &nbsp;&nbsp; 
			</div>
		  
		<!--   other buttons -->
		<!--    <div class="other"-->
	
			 
		
		
		  
		  <!-- End Form -->
		</form>
