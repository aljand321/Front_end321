<!DOCTYPE html>
<html lang="es">
<head>
	<title>Antecedentes</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<%- include("partials/head") -%>
</head>
<body class=" full-box dashboard-sideBar-imagg cover " >
	<!-- SideBar -->
	<%- include("partials/menudoc") -%>

	<li>
		<a href="/medico/renderVistaDocotr/<%=especialidad%>">
			<i class="zmdi zmdi-view-dashboard zmdi-hc-fw"></i> Home
		</a>
	</li>

    <li>
		<a href="#!" class="btn-sideBar-SubMenu">
			<i class="zmdi zmdi-file-plus zmdi-hc-fw"></i> Consultas externas <i class="zmdi zmdi-caret-down pull-right"></i>
		</a>
		<ul class="list-unstyled full-box">
			<li><a href="/medico/listfalseConsulta/<%=especialidad%>"><i class="zmdi zmdi-accounts-list zmdi-hc-fw"></i>Listas de citas</a></li>
					  
		</ul>
	</li>
	<%- include("partials/footerDocotr") -%>
	<!-- Content page-->
<section class="full-box dashboard-contentPage dashboard-sideBar-cttt">
	<!-- NavBar -->
	<nav class="full-box dashboard-Navbar dashboard-sideBar-imaggg cover ">
		<ul class="full-box list-unstyled text-right dashboard-sideBar-ctttt">
			<li class="pull-left">
		    	<a href="#!" class="btn-menu-dashboard"><i class="zmdi zmdi-more-vert"></i></a>
			</li>
			<li>
				<a href="#!" class="btn-Notifications-area">
					<i class="zmdi zmdi-notifications-none"></i>
				</a>
			</li>
		</ul>
	</nav>
		<!-- Content page -->
		<div class="container-fluid col-md-12" id="">
				<div class="page-header  col-md-6">	
					<h1 class="text-titles"><i class="zmdi zmdi-hospital zmdi-hc-fw"></i> Antecedentes<small></small></h1>  
			</div><br><br>
			<div class="btn-group  col-md-3">
				<button type="button" class="btn btn-info btn-raised"
					data-toggle="dropdown">
				  Detalles del Paciente <span class="caret"></span>
				</button>
				
				<ul class="dropdown-menu" role="menu">
					<li class="menu__item">
						<a href="/datos_generales_paciente/list_responsables/<%=dataPaciente[0].id%>" class="menu__link">Datos de responsable </a>
						
					</li>
					<li class="menu__item">
						<a href="/datos_generales_paciente/lista_alergias/<%=dataPaciente[0].id%>" class="menu__link">Alergia</a>						
					</li>
					<li class="menu__item">
						<a href="/datos_generales_paciente/list_exFisico/<%=dataPaciente[0].id%>" class="menu__link">Examen General</a>
					</li>
					<li class="divider"></li>
				  
				</ul>
			  </div> 
		   
			<div id="" class="col-md-2">
		
			  <div class="">
				<a href="/medico/volver" class="btn btn-success btn-raised btn-lg"><i class=""></i>Atras</a>	
			  </div>
			
			</div>
		</div>
	<!-- Antecedentes de consultas-->
	<div id="myTabContent" class="tab-content">
		<div class="tab-pane fade active in" id="new">
		  <div class="container-fluid">
			<div class="row">
			
			 		
				<p>Datos del Paciente</p>
				<div class="grupo" id="cabeza">
					
					<div id="cuatro">
						<div class="entrada">Historial:<br>
							  <label><font color="black"><%=dataPaciente[0].numeroHistorial%></font></label>
						</div>
						<div class="entrada">Apellido Paterno:<br>
							  <label><font color="black"><%=dataPaciente[0].apellidop%></font></label>
						</div>
						<div class="entrada">Apellido Materno:<br>
							  <label for=""><font color="black"><%=dataPaciente[0].apellidom%></font></label>
						</div>
						<div class="entrada">Nombre(s):<br>
							 <label for=""><font color="black"><%=dataPaciente[0].nombre%></font></label>
						</div>
					</div>
						
				</div>
				<br>
					<p>Antecedentes </p>
					<%if(update_paciente == null) { %>
					<form action="/datos_generales_paciente/antecedentes/<%=dataPaciente[0].id%>" method="POST"> 
						<div class="grupo" id="cabeza">
						  	<div id="cuatro">
								<div class="entrada">Fecha:<br>
							  		<input  type="date" name="fecha" require><br>
								</div>
								<div class="entrada">Antecedes de Familiares:<br>
									<input type="text" name="familiares" value="" size="20"><br>
							  	</div>
								<div class="entrada">Antecedes Patologicos:<br>
							  		<input type="text" name="personales_patologicos" size="20"><br>
								</div>
								<div class="entrada">Antecedes no Patologicos:<br>
							  		<input type="text" name="personales_no_patologicos" value=""size="10"><br>
								</div>
							</div>						
							<div id="tres2">
								<div class="entrada">Gineco Obstetrico:<br>
							  		<input type="text" name="gineco_obstetrico" value="" size="20"><br>
								</div>
								<div class="entrada">Descripción:<br>
									<textarea name="descripcion" id="" cols="50" rows="1"></textarea>
								</div>
							</div>
						</div>
						<br>
						<br>
						<p class="text-center">
						  	<button type="reset" class="btn btn-info btn-raised" style="margin-right: 20px;"><i class="zmdi zmdi-roller"></i> &nbsp;&nbsp; Limpiar</button>
						  	<button type="submit" class="btn btn-danger btn-raised"><i class="zmdi zmdi-floppy"></i> &nbsp;&nbsp; Guardar</button>
						</p>
					</form>
					<% } else { %>
						<h3>update</h3>
						<a href="/datos_generales_paciente/volver">Volver</a>
						<form action="/datos_generales_paciente/update_antecedentes/<%=update_paciente[0].id%>" method="POST"> 
							<div class="grupo" id="cabeza">
								  <div id="cuatro">
									<div class="entrada">Fecha:<br>
										  <input  type="date" name="fecha" value="<%=update_paciente[0].fecha%>"><br>
									</div>
									<div class="entrada">Antecedes de Familiares:<br>
										<input type="text" name="familiares" value="<%=update_paciente[0].familiares%>" size="20"><br>
									  </div>
									<div class="entrada">Antecedes Patologicos:<br>
										  <input type="text" name="personales_patologicos" value="<%=update_paciente[0].personales_patologicos%>" size="20"><br>
									</div>
									<div class="entrada">Antecedes no Patologicos:<br>
										  <input type="text" name="personales_no_patologicos" value="<%=update_paciente[0].personales_no_patologicos%>" size="10"><br>
									</div>
								</div>						
								<div id="tres2">
									<div class="entrada">Gineco Obstetrico:<br>
										  <input type="text" name="gineco_obstetrico" value="<%=update_paciente[0].gineco_obstetrico%>" size="20"><br>
									</div>
									<div class="entrada">Descripción:<br>
										<textarea name="descripcion" id="" cols="50" rows="1"><%=update_paciente[0].descripcion%></textarea>
									</div>
								</div>
							</div>
							<br>
							<br>
							<p class="text-center">
								  <button type="reset" class="btn btn-info btn-raised" style="margin-right: 20px;"><i class="zmdi zmdi-roller"></i> &nbsp;&nbsp; Limpiar</button>
								  <button type="submit" class="btn btn-danger btn-raised"><i class="zmdi zmdi-floppy"></i> &nbsp;&nbsp; Guardar</button>
							</p>
						</form>
					<% } %>
				<div class="table-responsive">
					<h3 class="text-center all-tittles">LISTA DE AlERGIAS</h3>
					<div class="table-wrapper">
						<div id="correo">
							<div class="entrada">
								<button type="button" value="Imprimir" onclick="window.print()" class="btn btn-success btn-raised btn-lg"><i class="zmdi zmdi-print"></i> &nbsp;&nbsp; Imprimir</button>
							</div>
							<div class="entrada">Buscar <i class="zmdi zmdi-search"></i><br>
								<input type="text" id="searchInput" placeholder="Buscar.."><br>
							</div>
						</div>
						<table class="fl-table" id="myTable">
							<thead>
								<tr>
								
								<th>Antecedentes familiares</th>
								<th>Antecedentes Patologicos</th>
								<th>Atecedentes no Patologicos</th>
								<th>Gineco Obstetrico</th>
								<th>Descripción</th>
								<th>Ver</th>
								
								</tr>
							</thead>
							<tbody>
								<% for(var i=0; i < list_antecedentes.length; i++) { %>
								<tr>									
									<td><%=list_antecedentes[i].familiares%></td>
									<td><%=list_antecedentes[i].personales_patologicos%></td>
									<td><%=list_antecedentes[i].personales_no_patologicos%></td>
									<td><%=list_antecedentes[i].gineco_obstetrico%></td>
									<td><%=list_antecedentes[i].descripcion%></td>
									<td><a href="/datos_generales_paciente/one_antecedente/<%=list_antecedentes[i].id%>" class="btn btn-info btn-raised btn-xs "><i class="zmdi zmdi-refresh-alt"></i></a></td>								
								</tr>
								<% } %>
							<tbody>
						</table>
					</div>
				</div>
				                                                                         
			                          
		  </div>
		</div>
	  </div>
	</div>
</section>
	<!--====== Scripts -->
	<%- include("partials/scripts") -%>
</body>
</html>