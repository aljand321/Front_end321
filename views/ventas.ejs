                                                    <tbody>
                                                </table>
                                            </div>
                                        </div>
                                        <!--Lista Transferidos-->
                                        <div class="tab-pane fade" id="listaD">
                                            <div class="container-fluid">
                                                <table class="fl-table">
                                                    <thead>
                                                        <tr>
                                                            <th>#</th>
                                                            <th>Historial Clinico</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    <tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>  
                                </div>   
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
	    </section>
	<!--====== Scripts -->
	<%- include("partials/scripts") -%>
