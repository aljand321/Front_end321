																			<option>TARDE</option>
																			<option>NOCHE</option>
																		</select>
																	</div>
																</div>
																<div class="col-md-4">
																	<div class="entrada">Consultorio / <span class="semi-bold">
																		<%=citaUpdate.data[0].especialidad%></span><br>
																		<select  class="form-control"name="especialidad" @click="especilida"
																			v-model="selectEsp">
																			<option
																				v-for="(especialidad, index) of especialidades">
																				{{especialidad.nombre}}</option>
																		</select>
																	</div>
																</div>
																
																<div class="col-md-4">
																	<div class="entrada">Médico / <span class="semi-bold"> <%=citaUpdate.data[0].medico%></span>
																		<br>
																		<select class="form-control" name="medico" v-model="selectDoctor"
																			@click="saca_horas">
																			<option disabled="disabled" value>Seleccione
																			</option>
																			<option v-for="(doctor, index) of doctores" :value="doctor.nombre">
																				{{doctor.nombre.split("/")[0]}}
																			</option>
																		</select>
																	</div>
																</div>
															
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-md-12 ">
													<div class="col-md-4"></div>
													<div v-if="horas_Turno == ''">
														<div class="col-md-4 ">
															<div class="block-web green-bg-color">
																<h3 class="content-header ">Hora de Reserva</h3>
																<input type="radio" name="hora" value="<%= citaUpdate.data[0].hora%>" checked><%= citaUpdate.data[0].hora.split("/")[0]  %><br>
															</div>
															<!--/block-web-->
														</div>
													</div>
													<div v-else>
														<div class="col-md-4 ">
															<div class="block-web blue-bg-color">
																<h3 class="content-header ">Cambiar hora </h3>
																<label for="">></label>
																<input type="radio" name="hora" value="<%= citaUpdate.data[0].hora%>" checked><%= citaUpdate.data[0].hora.split("/")[0]  %><br>
																<template v-for="(horas, index, key) in horas_Turno">
																	<label for="">{{index+1}}</label>
																	<input type="radio" name="hora" :value="horas.h +'/'+horas.id">{{horas.h}}<br>
																</template>
															</div>
															<!--/block-web-->
														</div>
													</div>
												</div>
											</div><!-- row de hora-->
											<div class="">
												<p class="col-md-6">
													<a href="/paciente/imprimir_2/<%=citaUpdate.data[0].id%>/<%=data_doc.data.token_id%>/<%=data_doc.data.token_p%>/<%=citaUpdate.data[0].codigo_p%>/<%=id%>" class="btn btn-warning btn-raised" style="margin-right: 20px;"> <i class='fa fa-print'></i> &nbsp;&nbsp;Imprimir</a> 
													<%if (citaUpdate.data[0].estado_update == true) { %>
														<button type="submit" class="btn btn-success btn-raised" style="margin-right: 20px;"><i class='fa fa-save'></i> &nbsp;&nbsp; Actualizar</button>
													<% } %>
													<a  href="/paciente/clean/<%=id%>/<%=data_doc.data.token_id%>/<%=data_doc.data.token_p%>/<%=citaUpdate.data[0].codigo_p%>" class="btn btn-info" >
														<i class="fa fa-mail-reply"></i> <span> Volver a Fichas</span> </a>
												</p>
											</div>		
										</div>
									</form>
							</div>
							<% } %>
							</div>
							</div>
