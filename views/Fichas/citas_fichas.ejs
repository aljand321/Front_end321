												<table class="display table table-bordered table-striped" id="dynamic-table2">
													<thead>
														<tr>
															<th>#</th>
															<th>Historial Clinico</th>
															<th>Médico</th>
															<th>Especialidad</th>
															<th>Fecha </th>
															<th>Saldo</th>
															<th>Hora</th>
															<th>Opciones</th>
														</tr>
													</thead>
													<tbody>
														<%if(pacienteCita != null) { %>
														<% for ( var i = 0; i < pacienteCita.length; i++ ) { %>
														<tr class="gradeX">
															<td><%=i+1%></td>
															<td><%=pacienteCita[i].codigo_p%></td>
															<td><%=pacienteCita[i].medico%></td>
															<td><%=pacienteCita[i].especialidad%></td>
															<td><%=pacienteCita[i].createdAt.split("T")[0]%></td>
															<td><%=pacienteCita[i].saldo_total%></td>
															<td><%=pacienteCita[i].hora.split("/")[0]%></td>
															<td class="center">
																<a href="/paciente/onliCita/<%=pacienteCita[i].id%>/<%=data_doc.data.token_id%>/<%=data_doc.data.token_p%>" data-toggle="tooltip" class="btn tooltips" data-original-title="Ver Cita"><i class="fa fa-eye "></i></a><span>&nbsp;</span>															
															</td>
														</tr>
														<% } %>
														<% } else { %>
														<tr>
															<td colspan="7"> Este paciente no tiene citas médicas</td>
														</tr>
														<% } %>
													</tbody>
												</table>
											</div>
										<div class="tab-pane fade " id="listt"></div>
										</div>
									</div>
								</section>
							</div>
						</div>
					</div>
				</div>
			</div>
