																		<input type="text" value="<%=citaUpdate.data[0].codigo_p%>" name="codigo_p" class="form-control" readonly="" value="Read only text goes here">
																	</div>
																	
																</div>
																<div class="col-md-4">
																	<div class="form-group">
																		<label> Paciente:</label>
																		<input type="text" value="<%=paciente[0].nombre%> <%=paciente[0].apellidop%> <%=paciente[0].apellidom%>" name="" class="form-control" readonly="" value="Read only text goes here">
