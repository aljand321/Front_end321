											<input type="hidden" name="id_user" value="<%=data_doc.data.personal.id%>">
											<input type="hidden" name="fecha" :value="fecha">
											<div class="row">
												<div class="col-md-12 ">
													<div class="block-web">
														<% if(msg_false != null) { %>
