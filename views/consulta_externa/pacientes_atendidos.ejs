                                                                <th>#</th>
                                                                <th>N° Historial</th>
                                                                <th>Nombre Paciente</th>
                                                                <th>Apellido Paterno</th>
                                                                <th>Apellido Materno</th>
                                                                <th>Fecha de Atencion</th>
                                                                <th>Estado</th>	   
                                                               
                                                            </tr>
                                                        </tfoot>
