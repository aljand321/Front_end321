                <h3 class="content-header"> <i class="fa fa-list icon"></i> Registrar Grupo de Asignacion</h3>
              </div>
              <div class="porlets-content">
              <% if(msg != null) { %>
                <h4 style="color:red;"><%=msg%></h4>
              <% } else { %>
                <h4 style="color:blue;"><%=pass%></h4>
              <% } %>
              <%if (oneGrupoAsig == null) { %>
              <form  class="form-horizontal group-border-dashed" action="/almacen/grupoAsig" method="POST" parsley-validate novalidate>
                <%if(err != null) { %>
                <div class="form-group">
                  <label class="col-sm-3 control-label">Codigo</label>
                  <div class="col-sm-6">                   
                    <input type="text" class="form-control" required parsley-minlength="3"  name="codigo" value="<%=err.codigo%>" />
                  </div>
                </div><!--/form-group--> 
                <div class="form-group">
                  <label class="col-sm-3 control-label">Descripcion</label>
                  <div class="col-sm-6">                   
                    <input type="text" class="form-control" required parsley-minlength="6" name="descripcion" value="<%=err.descripcion%>" />
                  </div>
                </div><!--/form-group--> 
                <% } else { %>
                  <div class="form-group">
                      <label class="col-sm-3 control-label">Codigo</label>
                      <div class="col-sm-6">                       
                        <input type="text" class="form-control" required parsley-minlength="3"  name="codigo" />
                      </div>
                    </div><!--/form-group--> 
                    <div class="form-group">
                      <label class="col-sm-3 control-label">Descripcion</label>
                      <div class="col-sm-6">
                        <input type="text" class="form-control" required parsley-minlength="6"  name="descripcion" />
                      </div>
                    </div><!--/form-group--> 
                <% } %>
                <div class="form-group">
                  <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-primary"><i class="fa fa-floppy-o"></i>  Guardar</button>
                    <button class="btn btn-danger"><i class="fa fa-times"></i> Cancelar</button>
                  </div>
                </div><!--/form-group--> 
              </form>
              <% } else { %>
              <form class="form-horizontal group-border-dashed" action="/almacen/updateGPA/<%=oneGrupoAsig[0].id%>" method="POST" parsley-validate novalidate>
                <div class="form-group">
                  <label class="col-sm-3 control-label">Codigo:</label>
                  <div class="col-sm-6">
                    <input type="text" class="form-control" value="<%=oneGrupoAsig[0].codigo%>" required parsley-minlength="3" name="codigo" />
                  </div>
                </div><!--/form-group--> 
                <div class="form-group">
                  <label class="col-sm-3 control-label">Descripcion:</label>
                  <div class="col-sm-6">
                    <input type="text" class="form-control" value="<%=oneGrupoAsig[0].descripcion%>"  required parsley-minlength="6" name="descripcion" />
                  </div>
