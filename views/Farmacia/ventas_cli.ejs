<!--Modal-->
<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="myModalLabel"><i class="fa fa-users icon"></i>Receta</h4>
        </div>
        <div class="modal-body"> 
          <form action="" method="POST" class="form-horizontal" role="form"  parsley-validate novalidate>
              <div class="col-md-3">
                <div class="entrada ">
                  <label class=" ">Codigo:</label><p><b>5156151</b></p>
                  <input type="hidden" name="numeroHistorial"  value="5156151"  class="form-control" readonly="" value="Read only text goes here">
                </div>
              </div>
              <div class="col-md-3">
                <div class="entrada ">
                  <label class=" ">Fecha:</label><p><b>01/20/2019</b></p>
                  <input type="hidden" name="numeroHistorial"  value="01/20/2019"  class="form-control" readonly="" value="Read only text goes here">
                </div>
              </div>
              <div class="col-md-5">
                <div class="entrada ">
                  <label class=" ">Encargada:</label><p><b>MARCO COLQUE VILLA</b></p>
                  <input type="hidden" name="numeroHistorial"  value="MARCO COLQUE VILLA"  class="form-control" readonly="" value="Read only text goes here">
                </div>
              </div>
              <div class="col-md-2">
                <div class="entrada ">
                  <label class=" ">C.I:</label><p><b>5156151</b></p>
                  <input type="hidden" name="numeroHistorial"  value="5156151"  class="form-control" readonly="" value="Read only text goes here">
                </div>
              </div>
              <div class="col-md-4">
                <div class="entrada ">
                  <label class=" ">Cliente:</label><p><b>LAURA ZAPATA LIMA</b></p>
                  <input type="hidden" name="numeroHistorial"  value="01/20/2019"  class="form-control" readonly="" value="Read only text goes here">
                </div>
              </div> 
            <div class="table-responsive">
                <table  class="display table table-bordered table-striped " >
                  <thead>
                    <tr>
                    <th>#</th>
                    <th>Cantidad del Producto</th>
                    <th>Codigo</th>
                    <th>Descripcion</th>
                    <th>Precio</th>
                    <th>Precio Total</th>
                    </tr>
                  </thead>
                  <tbody>                                                    
                                                                    
                  </tbody>
                </table>
              </div><!--/table-responsive-->
                  <div class="col-md-4">
                      <div class="form-group">
                        <div class="col-sm-9">
                          <div class="input-group"> <span class="input-group-addon">Total:</span>
                            <input type="text" class="form-control"  readonly :value="totalPrice + totalPrice * 0.18">
                          </div>
                        </div>
                      </div>
                    </div>
  
              
            <div class="form-group">
                <div class="col-sm-offset-0 col-sm-10">
                  <!--  <a type="button" class="btn btn-info" href="/Farmacia/reg_venta"> Aceptar</a> -->
                  <button type="submit" class="btn btn-success"><i class="fa fa-floppy-o"></i>  Guardar</button>
                  <button type="reset" class="btn btn-danger" data-dismiss="modal"><i class="fa fa-times"></i> Cancelar</button>
                </div>
              </div><!--/form-group--> 
            </form>
        </div>
      </div>
    </div>
  </div>
