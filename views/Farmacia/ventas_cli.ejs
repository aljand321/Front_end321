      <div class="contentpanel"><br>
		    <!--Formulario-->
		    <div class="container clear_both padding_fix">
          <!--\\\\\\\ table \\\\\\-->
          <div class="row">
            <div class="col-md-12">
              <div class="block-web">
                <div class="header">
                    <div class="actions"><a class="minimize" href="#">Volver</a> <a class="refresh" href="/Farmacia/ventas/<%=data_doc.data.token_id%>/<%=data_doc.data.token_p%>"><i class="fa fa-reply"></i></a> </div>
                    <h2 class="content-header"><div class="info gray_symbols"><i class="fa fa-shopping-cart icon"></i></div>   Ventas/<span class="semi-bold"> Lista de Ventas</span></h2>
                </div>
                <div class="porlets-content">
                  <div class="table-responsive">
                    <table  class="display table table-bordered table-striped " id="dynamic-table">
                      <thead>
                        <tr>
                        <th>#</th>
                        <th>C.I.</th>
                        <th>Cliente</th>
                        <th>Codigo Venta</th>
                        <th>Fecha</th>
                        <th>Total</th>
                        <th>Opciones</th>
                        </tr>
                      </thead>
                      <tbody>
                        <%for (var i = 0; i < resp.length; i++) { %>
                          <tr>
                            <td><%=i+1%></td>
                            <td><%=resp[i].ci%></td>
                            <% for (var j = 0; j < cliente.length; j++) {%>
                              <%if (cliente[j].ci == resp[i].ci) { %>
                                <td><%=cliente[j].nombre%>  <%=cliente[j].apellidop%> <%=cliente[j].apellidom%></td>
                              <% } %>
                            <% } %>
                            <td><%=resp[i].codigo_venta%></td>
                            <td><%=resp[i].createdAt.split("T")[0]%></td>
                            <td><%=resp[i].medicamentos.total%></td>
                            <td><a href="javascript:void(0);" data-toggle="modal" class="btn  tooltips" data-target="#myModal2" data-original-title="Ver Venta"><i class="fa fa-eye" style="color:rgb(22, 70, 202)"></i></a><span>&nbsp;</span>
                          </tr>
                        <% } %>
                      <tbody>
                    </table>
                  </div><!--/table-responsive-->
                </div><!--/porlets-content-->
              </div><!--/block-web--> 
            </div><!--/col-md-12--> 
          </div><!--/row-->
