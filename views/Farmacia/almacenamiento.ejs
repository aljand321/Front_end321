											<th>Codigo</th>
											<th>Nombre</th>
											<th>Unidad de Medida</th>
											<th>Forma Farmaceutica</th>
											<th>Cantidad en Unidad</th>
											<th>Fecha Llegada</th>
											<th>Fecha Vencimiento</th>
											<th>Precio Venta</th>
											<th>Opciones</th>
											</tr>
