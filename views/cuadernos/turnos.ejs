          <div class="panel-collapse collapse in" id="collapse4" style="height: auto;">
            <div class="panel-body">
              <%if (modifDoct == null) { %>
                <form class="form-horizontal group-border-dashed" action="/cuaderno/docCuaderno" method="POST" parsley-validate novalidate>
                  <div class="form-group">
                      
                      <div class="col-sm-6">
                          <label class="control-labe">Medico:</label>
                          <select class="form-control" id="source" name="nombre">
                                <%for (var i=0; i < resp.length; i++) { %>
                                    <option><%=resp[i].nombre%> <%=resp[i].apellidop%> <%=resp[i].apellidom%></option>
                                <% } %> 
                          </select>

                      </div>
                      
                      <div class="col-sm-6">
                          <label class="control-labe">Especialidad:</label>
                          <select class="form-control" id="source" name="especialidad">
                                <%for (var i=0; i < esp.length; i++) { %>
                                    <option><%=esp[i].nombre%></option>
                                <% } %> 
                          </select> 
                      </div>
                     
                  </div><!--/form-group--> 
                  <div class="form-group">
                        <div class="col-sm-6">
                            <label class="control-labe">Tipo de Medico:</label>
                            <select class="form-control" id="source" name="TDoctor">
                                    <option>Medico Titular</option>
                                    <option>Medico Suplente</option>
                            </select>
                        </div>
                        <div class="col-sm-6">
                            <label class="control-labe">Enfermera:</label>
                            <input type="text" name="enfermera" class="form-control" parsley-trigger="change"  name="nombre" required placeholder="Introdusca Nombre" />
                        </div>
                  </div>
                  <div class="form-group">
                      <div class="col-sm-offset-4 col-sm-10">
                        <button type="submit" class="btn btn-success btn-icon">Guardar <span>&nbsp;</span><i class="fa fa-check-square"></i></button><span>&nbsp;&nbsp;&nbsp;</span>
                        <button type="reset"class="btn btn-danger btn-icon">Cancelar <span>&nbsp;</span><i class="fa fa-times"></i></button>
                      </div>
                    </div><!--/form-group--> 
                  
                </form>
                <% } else { %>
                  <form class="form-horizontal group-border-dashed" action="/cuaderno/updateCuaderno/<%=OnlyC[0].id%>" method="POST" parsley-validate novalidate>
                    <div class="form-group">
                        
                        <div class="col-sm-4">
                            <label class="control-labe">Codigo:</label>
                          <input type="text" value="<%=OnlyC[0].id%>" class="form-control" parsley-trigger="change"  name="apellidop" required placeholder="Introdusca Apellido" />
                        </div>
                        
                        <div class="col-sm-4">
                            <label class="control-labe">Grupo de Asignacion:</label>
                          <input type="text"  name="grupo" value="<%=OnlyC[0].grupo%>" class="form-control" parsley-trigger="change" name="apellidom" required placeholder="Introdusca Apellido" />
                        </div>
                        <div class="col-sm-4">
                            <label class="control-labe">Titulo de Cuaderno:</label>
                          <input type="text" name="titulo" value="<%=OnlyC[0].titulo%>" class="form-control" parsley-trigger="change"  name="nombre" required placeholder="Introdusca Nombre" />
                        </div>
                    </div><!--/form-group--> 
                    
                    <div class="form-group">
                        <div class="col-sm-offset-4 col-sm-10">
                          <a class="btn btn-info active btn-icon" aria-pressed="true" role="button"  href="/cuaderno/limpiarC">Volver<span>&nbsp;</span><i class="fa fa-arrow-left"></i></a><span>&nbsp;&nbsp;&nbsp;</span>
                          <button type="submit" class="btn btn-success btn-icon">Guardar <span>&nbsp;</span><i class="fa fa-check-square"></i></button><span>&nbsp;&nbsp;&nbsp;</span>
                          <button type="reset"class="btn btn-danger btn-icon">Cancelar <span>&nbsp;</span><i class="fa fa-times"></i></button>
                          
                        </div>
                      </div><!--/form-group--> 
                    
                  </form>
                  <% } %>     
              
